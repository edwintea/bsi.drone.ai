set AZURE_STORAGE_ACCOUNT=rndgeneralstorage
set AZURE_STORAGE_KEY=wm5ByC5UJLK5w6jq80XzVuMEvsMoLqiLPu//SaV+O8uOzzVslzRXfn8sLr4NUAIw0gdrmD4wy6PM+ASt83PAcA==

az login --service-principal -u <appId> -p <password> --tenant <tenant>

azure cli command:

# Log in to Azure
az login

# Set the subscription (if needed)
az account set --subscription "My Subscription"

# List storage accounts
az storage account list --output table

# Get the storage account key
account_key=$(az storage account keys list --account-name mystorageaccount --query '[0].value' --output tsv)

# Create a container
az storage container create --name mycontainer --account-name mystorageaccount --account-key $account_key

# Upload a file
az storage blob upload --container-name imagedetection --file C:\Users\kubil\Pictures\Screenshots\test.png --name mytest.png --account-name rndgeneralstorage --account-key wm5ByC5UJLK5w6jq80XzVuMEvsMoLqiLPu//SaV+O8uOzzVslzRXfn8sLr4NUAIw0gdrmD4wy6PM+ASt83PAcA==

# List blobs in the container
az storage blob list --container-name imagedetection --account-name rndgeneralstorage --account-key wm5ByC5UJLK5w6jq80XzVuMEvsMoLqiLPu//SaV+O8uOzzVslzRXfn8sLr4NUAIw0gdrmD4wy6PM+ASt83PAcA== --output table


